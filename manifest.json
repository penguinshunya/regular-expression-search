{
  "manifest_version": 2,
  "name": "Regular Expression Search",
  "short_name": "regex search",
  "version": "2.2.0.0",
  "description": "search using regular expression on chrome extension",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      }
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/js/jquery.min.js",
        "lib/js/treap.js",
        "lib/js/rank-treap.js",
        "lib/js/function.js",
        "js/marker.js",
        "js/process.js",
        "js/search.js",
        "js/page.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "option.html",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*/*"
  ]
}
