{
  "manifest_version": 2,
  "name": "Regular Expression Search",
  "short_name": "regex search",
  "version": "2.5.3.3",
  "description": "__MSG_description__",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      }
    }
  },
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "lib/js/jquery.min.js",
      "lib/js/treap.js",
      "lib/js/rank-treap.js",
      "lib/js/function.js",
      "js/define.js",
      "js/marker.js",
      "js/search.js",
      "js/page.js"
    ]
  }],
  "background": {
    "scripts": [
      "lib/js/function.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "option.html",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*/*"
  ],
  "default_locale": "en",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
